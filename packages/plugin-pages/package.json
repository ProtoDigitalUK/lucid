{
	"name": "@lucidcms/plugin-pages",
	"version": "0.1.1",
	"description": "The official Pages plugin for Lucid",
	"type": "module",
	"types": "./dist/index.d.ts",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "tsc && tsup",
		"dev": "tsup --watch",
		"format": "biome format ./src",
		"format:fix": "biome format --write ./src",
		"lint": "biome lint ./src",
		"lint:fix": "biome lint --apply ./src"
	},
	"files": ["dist"],
	"author": "Proto Digital",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ProtoDigitalUK/lucid.git",
		"directory": "packages/plugin-pages"
	},
	"keywords": [
		"CMS",
		"Lucid CMS",
		"Lucid",
		"Proto Digital",
		"Pages",
		"Plugin",
		"Slug"
	],
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@lucidcms/config": "*",
		"@lucidcms/core": "*",
		"@types/node": "^22.1.0",
		"tsup": "^8.2.4",
		"typescript": "^5.5.4"
	},
	"peerDependencies": {
		"@lucidcms/core": "^0.7.0-alpha.0"
	},
	"volta": {
		"node": "20.15.0"
	},
	"license": "MIT"
}
